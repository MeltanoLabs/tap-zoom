{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "account_id": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "host_id": {
      "type": "string"
    },
    "id": {
      "type": "integer"
    },
    "recording_count": {
      "type": "integer"
    },
    "start_time": {
      "type": "string",
      "format": "date-time"
    },
    "topic": {
      "type": "string"
    },
    "total_size": {
      "type": "integer"
    },
    "type": {
      "type": "integer"
    },
    "uuid": {
      "type": "string"
    },
    "recording_play_passcode": {
      "type": "string"
    },
    "recording_files": {
      "type": "array",
      "items": {
          "type": "object",
          "properties": {
            "deleted_time": {
              "type": [
                "string",
                "null"
              ]
            },
            "download_url": {
              "type": "string"
            },
            "file_path": {
              "type": [
                "string",
                "null"
              ]
            },
            "file_size": {
              "type": "integer"
            },
            "file_type": {
              "type": "string"
            },
            "file_extension": {
              "type": "string"
            },
            "id": {
              "type": "string"
            },
            "meeting_id": {
              "type": "string"
            },
            "play_url": {
              "type": [
                "string",
                "null"
              ]
            },
            "recording_end": {
              "type": "string",
              "format": "date-time"
            },
            "recording_start": {
              "type": "string",
              "format": "date-time"
            },
            "recording_type": {
              "type": "string"
            },
            "status": {
              "type": "string"
            }
          },
          "required": [
            "download_url",
            "file_size",
            "file_type",
            "file_extension",
            "id",
            "meeting_id",
            "recording_end",
            "recording_start",
            "recording_type",
            "status"
          ]
        }
    }
  },
  "required": [
    "account_id",
    "duration",
    "host_id",
    "id",
    "recording_count",
    "start_time",
    "topic",
    "total_size",
    "type",
    "uuid",
    "recording_play_passcode",
    "recording_files"
  ]
}